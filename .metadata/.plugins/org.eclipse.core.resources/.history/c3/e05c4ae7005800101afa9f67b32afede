package testNGsample;

import org.testng.annotations.AfterSuite;
import org.testng.annotations.AfterTest;
import org.testng.annotations.BeforeClass;
import org.testng.annotations.BeforeTest;
import org.testng.annotations.DataProvider;
import org.testng.annotations.Test;

public class Day3 {
	
	@BeforeTest
	public void beforeTestExecute() {
		System.out.println("Before every Test");
	}
	
	@AfterTest
	public void afterTestExecyte() {
		System.out.println("After every test");
	}

	@AfterSuite
	public void afterSuite() {
		System.out.println("afterSuite");
	}
	
	@BeforeClass
	public void beforeClasstest() {
		System.out.println("beforeClassSuite");
	}
	
	@Test(dataProvider = "getData")
	public void webLoginCardLoan() {
		System.out.println("webLoginCardLoan");
	}
	
	@Test(dependsOnMethods={"apiLoginCardLaon"})
	public void mobileLoginCardlaon() {
		System.out.println("mobileLoginCardlaon");
	}
	
	@DataProvider
	public Object[][] getData()
	{
		Object[][] data = new Object[3][2];
		data[0][0] = "firstname1";
		data[0][1] = "password1";

		data[1][0] = "firstname2";
		data[1][1] = "password2";

		data[2][0] = "firstname3";
		data[2][1] = "password3";

		return data;
	}
	
	@Test
	public void apiLoginCardLaon() {
		System.out.println("apiLoginCardLaon");
	}

}
